</head>
<body>


<!-- Left Panel -->
<a href="javascript:" id="return-to-top"><i class="fa fa-arrow-up"></i></a>

<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="{{ path('home') }}">My Wod Journal</a>
    </div>
    <ul class="nav navbar-nav">
         <li class="active"><a href="{{ path('home') }}">Accueil</a></li>
         <li>  <a href="{{ path('community') }}">Communauté</a></li>
        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
       <li class="dropdown">
        <a class="dropdown-toggle" data-toggle="dropdown" href="#">Mon profil
        <span class="caret"></span></a>
        <ul class="dropdown-menu">
            <li> <a href="{{ path('wods_list') }}">Mes entrainements</a></li>
            <li><a href="{{ path('wod') }}">Ajouter un entrainement</a></li>
            <li><a href="{{ path('todo_list') }}">Entrainements à faire</a></li>
            <li><a href="{{ path('security_logout') }}"> Se déconnecter</a></li>
        </ul>
      </li>
      
      
        {% else %}
        <li> <a href="{{ path('security_login') }}">Se connecter</a></li>
        {% endif %}
        
    </ul>
  </div>
</nav>
</div>

<!-- Right Panel -->

<div class="container-fluid">

    <!-- Header-->
{% block body %}
    {% endblock %}

</div> <!-- .content -->

   <script>
                
                CKEDITOR.replace( 'wod[work]' );
     CKEDITOR.replace( 'wod[comment]' );
      CKEDITOR.replace( 'wod[weight]' );
      var work = document.getElementById('workout_value').innerHTML;
      var weights = document.getElementById('weights_value').innerHTML;
    CKEDITOR.instances['wod_work'].setData(work);
    CKEDITOR.instances['wod_weight'].setData(weights);
    </script>
